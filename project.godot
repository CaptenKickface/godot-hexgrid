"language": "GDScript",
"path": "res://demo/Hex.gd"
}, {
"base": "Node",
"class": "HexBoard",
"language": "GDScript",
"path": "res://HexMap.gd"
}, {
"base": "Node2D",
"class": "Piece",
"language": "GDScript",
"path": "res://Piece.gd"
}, {
"base": "Node2D",
"class": "Tile",
"language": "GDScript",
"path": "res://Tile.gd"
}, {
"base": "Piece",
"class": "Unit",
"language": "GDScript",
"path": "res://demo/Unit.gd"
} ]
_global_script_class_icons={
"Hex": "res://godot/Tile.png",
"HexBoard": "res://godot/HexMap.png",
"Piece": "res://godot/Piece.png",
"Tile": "res://godot/Tile.png",
"Unit": "res://godot/Piece.png"
}

[application]

config/name="hexgrid_demo"
